{
  "name": "vue-viewer-icons",
  "version": "0.1.2",
  "author": "Blame",
  "license": "MIT",
  "keywords": [
    "svg",
    "icons"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf src/components/icons/* && rimraf lib && rimraf es",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:icons": "rimraf src/components/icons/* && node generate.js",
    "build:lib": "rimraf lib && rimraf es && npm run build:icons && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@itwin/itwinui-icons": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.5"
  }
}
